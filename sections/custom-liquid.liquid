<section
  class="custom-liquid"
  data-section-id="{{ section.id }}"
>
  <div class="custom-liquid__wrapper">
      {{ section.settings.custom_liquid }}
  </div>
</section>

{% schema %}
{
  "name": "Custom liquid",
  "settings": [
    {
      "type": "liquid",
      "id": "custom_liquid",
      "label": "Custom liquid"
    }
  ],
  "presets": [
    {
      "name": "Custom liquid",
      "category": "Content"
    }
  ],
  "templates": [
    "index",
    "product",
    "page"
  ]
}

{% endschema %}